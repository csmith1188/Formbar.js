{
	"name": "formbar",
	"version": "3.0.0",
	"description": "Javascript rewrite of Formbar, an open source classroom management tool",
	"main": "app.js",
	"scripts": {
		"dev": "nodemon app",
		"start": "node app",
		"test": "jest",
		"init-db": "node database/init.js",
		"migrate": "node database/migrate.js",
		"format": "prettier --write \"**/*.js\"",
		"format:check": "prettier --check \"**/*.js\""
	},
	"_moduleAliases": {
		"@modules": "modules",
		"@middleware": "middleware",
		"@services": "services",
		"@controllers": "api/v1/controllers",
		"@errors": "errors"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/csmith1188/Formbar.js.git"
	},
	"license": "YCSTEL-1.0",
	"bugs": {
		"url": "https://github.com/csmith1188/Formbar.js/issues"
	},
	"homepage": "https://github.com/csmith1188/Formbar.js#readme",
	"nodemonConfig": {
		"ignore": [
			"settings.json",
			"logNumbers.json",
			"static/**"
		]
	},
	"dependencies": {
		"@datalust/winston-seq": "^3.0.1",
		"axios": "^1.12.2",
		"axios-cookiejar-support": "^6.0.4",
		"bcrypt": "^5.1.1",
		"cors": "^2.8.5",
		"dotenv": "^16.4.7",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"express-session": "^1.18.2",
		"google-auth-library": "^9.15.0",
		"handlebars": "^4.7.8",
		"jsonwebtoken": "^9.0.2",
		"module-alias": "^2.2.3",
		"node-fetch": "^3.3.2",
		"nodemailer": "^7.0.12",
		"passport": "^0.7.0",
		"passport-google-oauth20": "^2.0.0",
		"prettier": "^3.6.2",
		"querystring": "^0.2.1",
		"socket.io": "^4.5.4",
		"socket.io-client": "^4.8.1",
		"sqlite3": "^5.1.7",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"tough-cookie": "^5.1.2",
		"unzipper": "^0.12.3",
		"winston": "^3.11.0",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"jest": "^29.7.0",
		"nodemon": "^3.0.1",
		"supertest": "^7.1.0"
	}
}
